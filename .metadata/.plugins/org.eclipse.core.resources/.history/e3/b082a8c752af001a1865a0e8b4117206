package io;

import java.io.Serializable;

class Person implements Serializable {
	private String name;
	private int age;
	private char gender;

	public Person(String name, int age, char gender) {
		super();
		this.name = name;
		this.age = age;
		this.gender = gender;

	}

	@Override
	public String toString() {
		return "Person [name=" + name + ", age=" + age + ", gender=" + gender + "]";
	}

}

