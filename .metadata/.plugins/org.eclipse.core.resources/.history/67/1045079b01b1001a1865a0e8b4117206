package labexercises;

import java.io.FileInputStream;
import java.io.IOException;
import java.io.ObjectInputStream;
import java.util.HashMap;
import java.util.Map;

public class CourseReaderRunner {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Map<Integer, Course> courseSet = new HashMap<Integer, Course>();
	}

	public void readFile(Map<Integer, Course> set) throws IOException, ClassNotFoundException {
		FileInputStream fileInputStream = new FileInputStream("Courses.txt");

		ObjectInputStream objectInputStream = new ObjectInputStream(fileInputStream);
		Map<Integer, Course> readObject = (Map<Integer, Course>) objectInputStream.readObject();
		set = readObject;
	}
}
