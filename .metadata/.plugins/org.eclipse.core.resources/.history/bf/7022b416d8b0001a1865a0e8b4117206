package multithreading;

public class Participant extends Thread {
	private int ID;
	private String name;

	public Participant(int iD, String name) {
		ID = iD;
		this.name = name;
		System.out.println("Just joined the class!")
	}

	@Override
	public void run() {
		practice();
	}

	public void practice() {

		System.out.println("This is a good workout");
	}
}
